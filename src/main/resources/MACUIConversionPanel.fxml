<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefWidth="400.0" spacing="5.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <children>
            <HBox alignment="BASELINE_LEFT" spacing="5.0" VBox.vgrow="SOMETIMES">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Strand 1:" />
                  <TextField fx:id="strandOneTextField" promptText="Select file to populate" HBox.hgrow="SOMETIMES" />
               </children>
            </HBox>
            <HBox alignment="BASELINE_LEFT" spacing="5.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Strand 2:" />
                  <TextField fx:id="strandTwoTextField" promptText="Select file to populate" HBox.hgrow="SOMETIMES" />
               </children>
            </HBox>
            <HBox alignment="BASELINE_LEFT" spacing="5.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Selected File:" />
                  <Text fx:id="selectedFileName" strokeType="OUTSIDE" strokeWidth="0.0" />
               </children>
            </HBox>
            <HBox alignment="BASELINE_CENTER" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#selectScore6File" text="Select File" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
